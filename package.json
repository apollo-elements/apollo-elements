{
  "name": "root",
  "private": true,
  "type": "module",
  "logo": {
    "file": "./logo.png"
  },
  "scripts": {
    "build": "run-s build:*",
    "build:codegen": "graphql-codegen",
    "build:packages": "yarn workspaces run build",
    "build:analyze": "yarn workspaces run analyze",
    "clean": "run-s clean:*",
    "clean:packages": "rimraf 'packages/!(create)/**/!(rollup.config).{js,js.map,d.ts,tgz}'",
    "clean:create-app": "rimraf 'packages/create/*.{js,js.map,d.ts,tgz}'",
    "clean:node-modules": "rimraf 'packages/**/node_modules'",
    "release": "sh scripts/release.sh",
    "release:prerelease": "sh scripts/prerelease.sh",
    "postrelease": "yarn clean",
    "docs": "run-s build:codegen docs:*",
    "docs:analyze": "node --input-type module -e \"await import('./packages/docs/rocket-plugins/copy-manifests.js').then(m => m.generateManifests())\"",
    "x-not-ready-yet-docs:analyze": "custom-elements-manifest analyze",
    "docs:rollup": "rollup -c rollup.config.docs.js",
    "docs:esbuild": "node ./scripts/build-components.js",
    "docs:rocket": "ELEVENTY_ENV=production rocket build",
    "docs:copy": "run-s docs:copy:*",
    "docs:copy:assets": "cpx 'docs/_assets/**/*.{svg,png,jpg}' _site/_merged_assets/",
    "docs:copy:script": "cpx 'docs/_assets/**/*.*' _site/_assets/",
    "docs:copy:playground": "cpx 'node_modules/playground-elements/**/*' _site/_assets/_static/playground-elements",
    "lint": "run-s lint:*",
    "lint:clean": "rimraf packages/gluon/declaration.d.ts",
    "lint:eslint": "eslint .",
    "lint:types": "yarn workspaces run lint:types",
    "postinstall": "patch-package",
    "start": "run-p start:*",
    "start:clean": "yarn clean",
    "start:codegen": "graphql-codegen --watch",
    "start:tsc": "yarn workspaces run start --stream --parallel",
    "start:docs": "yarn docs:esbuild && rocket start",
    "test": "wtr --coverage",
    "watch:test": "wtr --watch",
    "watch:eslint": "esw",
    "watch:codegen": "graphql-codegen --watch",
    "watch:types": "yarn workspaces run tsc --watch --noEmit"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "git add"
    ]
  },
  "workspaces": [
    "packages/*",
    "docs"
  ],
  "devDependencies": {
    "@11ty/eleventy-plugin-inclusive-language": "^1.0.2",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.1.0",
    "@airbnb/node-memwatch": "^2.0.0",
    "@apollo-elements/rollup-plugin-graphql": "^1.0.3",
    "@borgar/textbox": "^1.2.1",
    "@changesets/cli": "^2.16.0",
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@custom-elements-json/helpers": "^0.0.1",
    "@custom-elements-manifest/analyzer": "^0.1.8",
    "@gluon/gluon": "^2.5.3",
    "@gluon/keybinding": "^1.1.0",
    "@graphql-codegen/cli": "^1.21.3",
    "@graphql-codegen/typed-document-node": "^1.18.4",
    "@graphql-codegen/typescript": "^1.21.1",
    "@graphql-codegen/typescript-operations": "^1.17.15",
    "@jlengstorf/get-share-image": "^0.8.0",
    "@material/mwc-button": "^0.20.0",
    "@open-wc/testing": "^3.0.0-next.1",
    "@polymer/iron-component-page": "^4.0.1",
    "@polymer/polymer": "^3.4.1",
    "@power-elements/json-viewer": "^2.1.1",
    "@pwrs/eslint-config": "^0.0.20",
    "@rocket/blog": "^0.3.0",
    "@rocket/cli": "^0.6.3",
    "@rocket/launch": "^0.4.2",
    "@rocket/navigation": "^0.2.1",
    "@rocket/search": "^0.3.5",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-node-resolve": "11.2.1",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/chai": "^4.2.16",
    "@types/graphql": "^14.5.0",
    "@types/mkdirp": "^1.0.1",
    "@types/ncp": "^2.0.4",
    "@types/prompts": "^2.0.10",
    "@typescript-eslint/parser": "^4.21.0",
    "@web/dev-server": "^0.1.17",
    "@web/dev-server-esbuild": "^0.2.11",
    "@web/dev-server-import-maps": "^0.0.5",
    "@web/dev-server-rollup": "^0.3.2",
