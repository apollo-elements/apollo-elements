    "get-github-url": "^1.0.4",
    "globby": "^11.0.3",
    "graphql": "^15.4.0",
    "graphql-tag": "^2.12.4",
    "graphql-tools": "^7.0.2",
    "hast-util-from-parse5": "^6.0.1",
    "hast-util-to-html": "^7.1.2",
    "hastscript": "^7.0.0",
    "husky": "^6.0.0",
    "hybrids": "^5.2.1",
    "jsdom": "^16.5.2",
    "lint-staged": "^10.5.4",
    "lit": "^2.0.0-rc.1",
    "lit-element": "^2.4.0",
    "lit-html": "^1.4.0",
    "markdown-it": "^12.0.4",
    "markdown-it-prism": "^2.1.6",
    "mermaid": "^8.9.2",
    "mermaid.cli": "^0.5.1",
    "node-fetch": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "parse5": "^6.0.1",
    "patch-package": "^6.4.7",
    "playground-elements": "^0.9.2",
    "ramda": "^0.27.1",
    "remark-mermaid": "^0.2.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.46.0",
    "rollup-plugin-esbuild": "^4.2.3",
    "rollup-plugin-lit-css": "^2.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sanitize-html": "^2.3.3",
