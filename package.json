    "typedoc": "^0.20.35",
    "typescript": "^4.2.4",
    "unist-util-map": "^3.0.0",
    "web-component-analyzer": "^1.1.6",
    "woff2base64": "^2.0.0"
  },
  "dependencies": {
    "tslib": "^2.2.0"
  },
  "resolutions": {
    "@11ty/eleventy": "^0.11",
    "graphql": "^15.4.0"
  }
}
