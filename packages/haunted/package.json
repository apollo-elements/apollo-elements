{
  "name": "@apollo-elements/haunted",
  "version": "2.0.1-next.0",
  "description": "👩‍🚀 Haunted Hooks for Apollo GraphQL 🌛",
  "main": "index.js",
  "module": "index.js",
  "type": "module",
  "typings": "index.d.ts",
  "exports": {
    ".": {
      "esbuild": "./index.ts",
      "default": "./index.js"
    },
    "./*": {
      "esbuild": "./*.ts",
      "default": "./*.js"
    },
    "./*.js": {
      "esbuild": "./*.ts",
      "default": "./*.js"
    }
  },
  "files": [
    "custom-elements.json",
    "*.d.ts",
    "*.js",
    "*.js.map",
    "**/*.d.ts",
    "**/*.js",
    "**/*.js.map"
  ],
  "scripts": {
    "prepublishOnly": "tsc -b .",
    "build": "tsc",
    "analyze": "custom-elements-manifest analyze",
    "lint": "run-s lint:*",
    "lint:types": "tsc --noEmit",
    "start": "tsc -w --noEmit",
    "test": "echo \"Please run tests from the repository root\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/apollo-elements/apollo-elements.git",
    "directory": "packages/haunted"
  },
  "keywords": [
    "Apollo",
    "GraphQL",
    "Web Components",
    "Custom Elements",
    "Haunted"
  ],
  "author": "Benny Powers <web@bennypowers.com>",
  "license": "ISC",
  "homepage": "https://apolloelements.dev/api/libraries/haunted/",
  "dependencies": {
    "@apollo-elements/core": "^1.0.1-next.0",
    "haunted": "^4.8.1"
  },
  "customElements": "custom-elements.json"
}
