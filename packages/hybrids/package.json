{
  "name": "@apollo-elements/hybrids",
  "version": "4.0.0",
  "description": "üë©‚ÄçüöÄ Hybrids Starter Cultures for Apollo GraphQL üåõ",
  "main": "index.js",
  "module": "index.js",
  "type": "module",
  "typings": "index.d.ts",
  "exports": {
    ".": {
      "esbuild": "./index.ts",
      "default": "./index.js"
    },
    "./*": {
      "esbuild": "./*.ts",
      "default": "./*.js"
    },
    "./*.js": {
      "esbuild": "./*.ts",
      "default": "./*.js"
    }
  },
  "files": [
    "custom-elements.json",
    "*.d.ts",
    "*.js",
    "*.js.map",
    "**/*.d.ts",
    "**/*.js",
    "**/*.js.map"
  ],
  "scripts": {
    "prepublishOnly": "tsc -b .",
    "analyze": "custom-elements-manifest analyze",
    "test": "wtr --coverage --config ../../web-test-runner.config.js  --root-dir '../..' './*.test.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/apollo-elements/apollo-elements.git",
    "directory": "packages/hybrids"
  },
  "keywords": [
    "Apollo",
    "GraphQL",
    "Web Components",
    "Custom Elements",
    "Hybrids"
  ],
  "author": "Benny Powers <web@bennypowers.com>",
  "license": "ISC",
  "homepage": "https://apolloelements.dev/api/libraries/hybrids",
  "dependencies": {
    "@apollo-elements/core": "^1.0.0",
    "@lit/reactive-element": "^1.0.2",
    "hybrids": "^7",
    "tslib": "^2.3.1"
  },
  "customElements": "custom-elements.json"
}
